object "C_7" {
    code {
        {
            if callvalue() { revert(0, 0) }
            let _1 := datasize("C_7_deployed")
            codecopy(128, dataoffset("C_7_deployed"), _1)
            return(128, _1)
        }
    }
    object "C_7_deployed" {
        code { { revert(0, 0) } }
    }
}

